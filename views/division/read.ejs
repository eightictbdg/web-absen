<h1 class="merah">Division</h1>
<div class="bar"></div>
<br>
ID: <%= division.id %> <br>
Name: <%= division.name %> <br>
Users: <br>
<% division.users.forEach(function(user){ %>
  - <a href="/panel/admin/user/<%= user.id %>"><%= user.name %> </a><br>
<% }) %>
<a href="<%= division.id %>/update"><button class="btn btn-success">Edit</button></a> <br>
<a href="<%= division.id %>/delete"><button class="btn btn-danger">Delete</button></a>